{
  "name": "@myparcel/eslint-config",
  "author": "Edie Lemoine (edie@myparcel.nl)",
  "license": "GPL-3.0",
  "version": "1.2.1",
  "description": "ESLint configurations for various types of projects.",
  "scripts": {
    "build": "node src/createFiles.js",
    "lint": "eslint --fix ./*.js",
    "test": "jest --coverage",
    "build:watch": "npm-watch build",
    "release": "node ./private/release.js",
    "prepublishOnly": "npm run test && npm run build",
    "link": "npm run build && npm link"
  },
  "main": "index.js",
  "files": [
    "tsconfig.json",
    "{plugin,preset}-*.js",
    "src/configurations/**/*.js",
    "!*.template.*"
  ],
  "watch": {
    "build": {
      "patterns": [
        "src"
      ]
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-template": "^0.3.2",
    "eslint-plugin-vue": "^6.2.1",
    "eslint-plugin-you-dont-need-momentjs": "^1.5.0",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@commitlint/cli": "^8.3.5",
    "@types/jest": "^25.1.3",
    "babel-jest": "^25.1.0",
    "conventional-github-releaser": "^3.1.3",
    "coveralls": "^3.0.9",
    "github-release-from-changelog": "^2.1.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-runner-eslint": "^0.7.6",
    "npm-watch": "^0.6.0",
    "standard-version": "^7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myparcelnl/eslint-config.git"
  },
  "resolutions": {
    "@babel/parser": "7.7.5"
  },
  "bugs": {
    "url": "https://github.com/myparcelnl/eslint-config/issues"
  },
  "homepage": "https://github.com/myparcelnl/eslint-config#readme",
  "keywords": [
    "eslint"
  ]
}
