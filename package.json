    "@myparcel/semantic-release-config": "^1.2.3",
    "@types/jest": "^27.0.3",
    "babel-jest": "^27.4.0",
    "jest": "^27.4.0",
    "jest-runner-eslint": "^1.0.0",
    "vue-jest": "^3.0.7",
    "watch": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myparcelnl/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/myparcelnl/eslint-config/issues"
